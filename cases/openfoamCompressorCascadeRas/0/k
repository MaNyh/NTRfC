 FoamFile {
    version 2.0;
    format ascii;
    class volScalarField;
    object k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //



kInlet <PARAM tke_inlet PARAM>;
dimensions [0 2 -2 0 0 0 0];

internalField uniform $kInlet;


boundaryField {
    INLET
    {
        type fixedValue;
        value uniform $kInlet;
    }
    BLADE
    {
        type kqRWallFunction;
        value uniform $kInlet;
    }
    OUTLET
    {
        type inletOutlet;
        inletValue uniform 1;
        value uniform $kInlet;
    }
    Z_lower
    {
        type            cyclic;
    }
    Z_upper
    {
        type            cyclic;
    }
    Y_lower
    {
        type            cyclic;
    }
    Y_upper
    {
        type            cyclic;
    }
}
